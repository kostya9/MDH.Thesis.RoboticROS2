<launch>
    <include file="$(find gazebo_ros)/launch/gazebo.launch.py">     
        <let name="world" value="./world.world"/>      
    </include>

    <group>
        <push_ros_namespace namespace="swarmbot0"/>
        <include file="./onerobot.launch.xml">
            <let name="robot_name" value="swarmbot0"/>
            <let name="init_pose_x" value="-12.0"/>  
            <let name="init_pose_y" value="-12.0"/>  
            <let name="init_pose_z" value="0"/>  
        </include>
    </group>
 
</launch>